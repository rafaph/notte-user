    "argon2": "^0.31.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "jose": "^4.14.4",
    "knex": "^2.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.6.0",
    "nest-knexjs": "^0.0.17",
    "nestjs-pino": "^3.3.0",
    "pino-http": "^8.4.0",
