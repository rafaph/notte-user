{
  "name": "notte-user",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "start:dev": "TS_NODE_PROJECT=\"./tsconfig.cov.json\" ts-node-dev -r tsconfig-paths/register src/main.ts | pino-pretty",
    "start:dev:test": "ts-node -r tsconfig-paths/register src/main.ts",
    "test": "mocha",
    "pretest:cov": "rm -rf coverage .nyc_output",
    "test:cov": "TS_NODE_PROJECT=\"./tsconfig.cov.json\" nyc npm run test",
    "test:e2e": "npm run test -- --grep @integration",
    "test:unit": "npm run test:e2e -- --invert",
    "prelint": "ls-lint",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "check-updates": "npx --quiet --yes npm-check-updates"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@ls-lint/ls-lint": "^2.0.1",
    "@swc/core": "^1.3.69",
    "@types/async": "^3.2.20",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.5",
    "@types/chai-arrays": "^2.0.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-datetime": "^0.0.37",
    "@types/chai-subset": "^1.3.3",
    "@types/cors": "^2.8.13",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.19",
    "@types/pg": "^8.10.2",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@types/swagger-ui-dist": "^3.30.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "async": "^3.2.4",
    "axios": "^1.4.0",
    "chai": "^4.3.7",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.8.0",
    "chai-subset": "^1.6.0",
    "connection-string": "^4.3.6",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "get-port-please": "^3.0.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "mocha.parallel": "^0.15.6",
    "nyc": "^15.1.0",
    "openapi-types": "^12.1.3",
    "pino-pretty": "^10.0.1",
    "prettier": "^3.0.0",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "0http": "^3.5.2",
    "argon2": "^0.30.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "http-status": "^1.6.2",
    "inversify": "^6.0.1",
    "low-http-server": "^4.1.0",
    "oxide.ts": "^1.1.0",
    "pg": "^8.11.1",
    "pino": "^8.14.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-dist": "^5.1.1",
    "zod": "^3.21.4"
  }
}
